#!/bin/bash

## DEBUGGING
DEBUG=${DEBUG:-false}

## CORE
OWNCLOUD_FQDN=${OWNCLOUD_FQDN:-$OWNCLOUD_PHP_FPM_ENV_OWNCLOUD_FQDN}
OWNCLOUD_FQDN=${OWNCLOUD_FQDN:-localhost}
OWNCLOUD_UPLOAD_MAX_FILESIZE=${OWNCLOUD_UPLOAD_MAX_FILESIZE:-4G}
OWNCLOUD_MAX_FILE_UPLOADS=${OWNCLOUD_MAX_FILE_UPLOADS:-20}

## DATABASE
DB_TYPE=${DB_TYPE:-}
DB_HOST=${DB_HOST:-}
DB_PORT=${DB_PORT:-}
DB_NAME=${DB_NAME:-}
DB_USER=${DB_USER:-}
DB_PASS=${DB_PASS:-}
